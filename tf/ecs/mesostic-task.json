[
  "image": "docker.io/maroda/chaquo:hpschd_v1.2.0",
  "name": "hpschd",
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/mesostic",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 9999,
          "protocol": "tcp",
          "containerPort": 9999
        }
      ]
    }
]
